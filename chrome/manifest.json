{
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon_dict.png",
        "default_popup": "options.html"
    },
    "content_scripts": [{
        "all_frames": true,
        "js": ["lookup.js"],
        "css": ["lookup.css"],
        "match_about_blank": true,
        "matches": ["<all_urls>"],
        "run_at": "document_end"
    }],
    "icons": {
        "16": "icon_dict.png",
        "48": "icon_dict.png",
        "128": "icon_dict.png"
    },
    "manifest_version": 2,
    "name": "饭否划词翻译",
    "short_name": "饭译",
    "version": "0.1.0",
    "description": "饭否划词翻译 Powered by @home2",
    "options_page": "options.html",
    "permissions": ["activeTab"],
    "web_accessible_resources": ["query.png", "speech.png"]
}